{
  "name": "@bagaar/eslint-config",
  "version": "0.6.10",
  "description": "EsLint config for Bagaar projects.",
  "keywords": [
    "bagaar",
    "bgr",
    "config",
    "eslint",
    "eslintconfig"
  ],
  "repository": "https://github.com/Bagaar/eslint-config",
  "license": "MIT",
  "author": "Bagaar",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "format": "prettier --check lib",
    "format:fix": "prettier --write lib",
    "lint": "eslint . --cache",
    "prepare": "husky install",
    "release": "standard-version",
    "test": "eslint tests",
    "test:debug": "DEBUG=true eslint tests"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": "prettier --write"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-decorators": "^7.22.3",
    "@commitlint/cli": "18.4.0",
    "@commitlint/config-conventional": "18.4.0",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "release-plan": "^0.8.0",
    "standard-version": "^9.5.0"
  },
  "peerDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.3",
    "@graphql-eslint/eslint-plugin": "^3.19.1",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-ember": "^12.0.0",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-qunit": "^8.0.0",
    "typescript": "^5.3.3"
  },
  "peerDependenciesMeta": {
    "@babel/eslint-parser": {
      "optional": true
    },
    "@babel/plugin-proposal-decorators": {
      "optional": true
    },
    "@graphql-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/eslint-plugin": {
      "optional": true
    },
    "@typescript-eslint/parser": {
      "optional": true
    },
    "eslint-plugin-ember": {
      "optional": true
    },
    "eslint-plugin-n": {
      "optional": true
    },
    "eslint-plugin-qunit": {
      "optional": true
    },
    "typescript": {
      "optional": true
    }
  },
  "volta": {
    "node": "20.10.0",
    "pnpm": "8.11.0"
  }
}
